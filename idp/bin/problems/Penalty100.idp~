penalty(199, 36) <- ~(( there_is_a_knife & dinner ) => ( false)).
penalty(200, 26) <- ~(( there_is_a_knife | there_is_a_spoon)).
penalty(201, 26) <- ~(( there_is_a_smallKnife ) => ( there_is_a_knife)).
penalty(202, 26) <- ~(( there_is_a_plate ) => ( there_is_a_knife)).
