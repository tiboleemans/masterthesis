penalty(198, 1) <- ~( !X, Y : ( above_of(X,Y) & pastaPlate(Y) ) => ( false)).
penalty(199, 1) <- ~( !X, Y : ( above_of(X,Y) & soupPlate(Y) ) => ( false)).
penalty(200, 1) <- ~( !X, Y : ( above_of(X,Y) & above_of(Y,X) ) => ( false)).
penalty(201, 1) <- ~( !X, Y : ( above_of(X,Y) & on(Y,X) ) => ( false)).
penalty(202, 1) <- ~( !X, Y : ( above_of(X,Y) ) => ( food_utensil(X))).
