penalty(2114, 1) <- ~( !W, X, Y, Z : ( on(W,X) & left_of(X,Y) & left_of(Y,Z) & on(Y,Z) ) => ( false)).
penalty(2115, 1) <- ~( !W, X, Y, Z : ( on(W,X) & left_of(X,Y) & left_of(Y,Z) & on(Z,W) ) => ( false)).
penalty(2116, 1) <- ~( !W, X, Y, Z : ( on(W,X) & left_of(X,Y) & left_of(Y,Z) & on(Z,X) ) => ( false)).
penalty(2117, 1) <- ~( !W, X, Y, Z : ( on(W,X) & left_of(X,Y) & left_of(Y,Z) & on(Z,Y) ) => ( false)).
penalty(2118, 1) <- ~( !W, X, Y, Z : ( on(W,X) & left_of(X,Y) & left_of(Y,Z) ) => ( food_utensil(Z))).
penalty(2119, 1) <- ~( !W, X, Y, Z : ( on(W,X) & left_of(X,Y) & left_of(Y,Z) & food_container(Z) ) => ( false)).
penalty(2120, 1) <- ~( !W, X, Y, Z : ( on(W,X) & left_of(X,Y) & left_of(Y,Z) & drink_container(Z) ) => ( false)).
penalty(2121, 1) <- ~( !X, Y : ( food_utensil(X) & left_of(X,Y) & fork(Y) ) => ( fork(X))).
penalty(2122, 1) <- ~( !X, Y : ( food_utensil(X) & left_of(X,Y) & plate(Y) ) => ( fork(X))).
penalty(2123, 1) <- ~( !X, Y : ( food_utensil(X) & left_of(X,Y) & bowl(Y) ) => ( fork(X))).
penalty(2124, 1) <- ~( !X, Y : ( food_utensil(X) & left_of(X,Y) & soupPlate(Y) ) => ( fork(X))).
penalty(2125, 1) <- ~( !X, Y : ( food_utensil(X) & left_of(X,Y) & spoon(Y) ) => ( knife(X))).
penalty(2126, 1) <- ~( !X, Y : ( food_utensil(X) & left_of(X,Y) & pastaPlate(Y) ) => ( spoon(X))).
penalty(2127, 1) <- ~( !X, Y, Z : ( food_utensil(X) & left_of(X,Y) & knife(Y) & left_of(Y,Z) ) => ( false)).
penalty(2128, 1) <- ~( !X, Y, Z : ( food_utensil(X) & left_of(X,Y) & spoon(Y) & left_of(Y,Z) ) => ( false)).
penalty(2129, 1) <- ~( !X, Y, Z : ( food_utensil(X) & left_of(X,Y) & left_of(Y,Z) & spoon(Z) ) => ( false)).
penalty(2130, 1) <- ~( !X, Y, Z : ( food_utensil(X) & left_of(X,Y) & left_of(Y,Z) & smallKnife(Z) ) => ( false)).
penalty(2131, 1) <- ~( !X, Y, Z : ( food_utensil(X) & left_of(X,Y) & left_of(Y,Z) & bowl(Z) ) => ( false)).
penalty(2132, 1) <- ~( !X, Y, Z : ( food_utensil(X) & left_of(X,Y) & left_of(Y,Z) & saucer(Z) ) => ( false)).
penalty(2133, 1) <- ~( !X, Y, Z : ( food_utensil(X) & left_of(X,Y) & left_of(Y,Z) & pastaPlate(Z) ) => ( false)).
penalty(2134, 1) <- ~( !X, Y, Z : ( food_utensil(X) & left_of(X,Y) & left_of(Y,Z) & soupPlate(Z) ) => ( false)).
penalty(2135, 1) <- ~( !X, Y, Z : ( food_utensil(X) & left_of(X,Y) & left_of(Y,Z) & waterGlass(Z) ) => ( false)).
penalty(2136, 1) <- ~( !X, Y, Z : ( food_utensil(X) & left_of(X,Y) & left_of(Y,Z) & wineGlass(Z) ) => ( false)).
penalty(2137, 1) <- ~( !X, Y, Z : ( food_utensil(X) & left_of(X,Y) & left_of(Y,Z) & teaCup(Z) ) => ( false)).
penalty(2138, 1) <- ~( !X, Y, Z : ( food_utensil(X) & left_of(X,Y) & left_of(Y,Z) & above_of(X,Z) ) => ( false)).
penalty(2139, 1) <- ~( !X, Y, Z : ( food_utensil(X) & left_of(X,Y) & left_of(Y,Z) & above_of(Y,Z) ) => ( false)).
penalty(2140, 1) <- ~( !X, Y, Z : ( food_utensil(X) & left_of(X,Y) & left_of(Y,Z) & above_of(Z,Y) ) => ( false)).
penalty(2141, 1) <- ~( !X, Y, Z : ( food_utensil(X) & left_of(X,Y) & left_of(Y,Z) & on(Y,Z) ) => ( false)).
penalty(2142, 1) <- ~( !X, Y, Z : ( food_utensil(X) & left_of(X,Y) & left_of(Y,Z) & on(Z,Y) ) => ( false)).
penalty(2143, 1) <- ~( !X, Y, Z : ( food_utensil(X) & left_of(X,Y) & left_of(Y,Z) & drink_container(Z) ) => ( false)).
penalty(2144, 1) <- ~( !X, Y : ( food_utensil(X) & left_of(X,Y) ) => ( food_utensil(Y) | food_container(Y))).
penalty(2145, 1) <- ~( !X, Y, Z : ( food_container(X) & left_of(X,Y) & spoon(Y) & left_of(Y,Z) ) => ( false)).
penalty(2146, 1) <- ~( !X, Y, Z : ( food_container(X) & left_of(X,Y) & left_of(Y,Z) & fork(Z) ) => ( false)).
penalty(2147, 1) <- ~( !X, Y, Z : ( food_container(X) & left_of(X,Y) & left_of(Y,Z) & smallKnife(Z) ) => ( false)).
penalty(2148, 1) <- ~( !X, Y, Z : ( food_container(X) & left_of(X,Y) & left_of(Y,Z) & plate(Z) ) => ( false)).
penalty(2149, 1) <- ~( !X, Y, Z : ( food_container(X) & left_of(X,Y) & left_of(Y,Z) & bowl(Z) ) => ( false)).
penalty(2150, 1) <- ~( !X, Y, Z : ( food_container(X) & left_of(X,Y) & left_of(Y,Z) & saucer(Z) ) => ( false)).
penalty(2151, 1) <- ~( !X, Y, Z : ( food_container(X) & left_of(X,Y) & left_of(Y,Z) & pastaPlate(Z) ) => ( false)).
penalty(2152, 1) <- ~( !X, Y, Z : ( food_container(X) & left_of(X,Y) & left_of(Y,Z) & soupPlate(Z) ) => ( false)).
penalty(2153, 1) <- ~( !X, Y, Z : ( food_container(X) & left_of(X,Y) & left_of(Y,Z) & waterGlass(Z) ) => ( false)).
penalty(2154, 1) <- ~( !X, Y, Z : ( food_container(X) & left_of(X,Y) & left_of(Y,Z) & wineGlass(Z) ) => ( false)).
penalty(2155, 1) <- ~( !X, Y, Z : ( food_container(X) & left_of(X,Y) & left_of(Y,Z) & teaCup(Z) ) => ( false)).
penalty(2156, 1) <- ~( !W, X, Y, Z : ( food_container(W) & left_of(W,X) & left_of(X,Y) & left_of(Y,Z) ) => ( false)).
penalty(2157, 1) <- ~( !X, Y, Z : ( food_container(X) & left_of(X,Y) & left_of(Y,Z) & above_of(Y,Z) ) => ( false)).
penalty(2158, 1) <- ~( !X, Y, Z : ( food_container(X) & left_of(X,Y) & left_of(Y,Z) & above_of(Z,Y) ) => ( false)).
penalty(2159, 1) <- ~( !X, Y, Z : ( food_container(X) & left_of(X,Y) & left_of(Y,Z) & on(X,Z) ) => ( false)).
penalty(2160, 1) <- ~( !X, Y, Z : ( food_container(X) & left_of(X,Y) & left_of(Y,Z) & on(Y,Z) ) => ( false)).
penalty(2161, 1) <- ~( !X, Y, Z : ( food_container(X) & left_of(X,Y) & left_of(Y,Z) & on(Z,X) ) => ( false)).
penalty(2162, 1) <- ~( !X, Y, Z : ( food_container(X) & left_of(X,Y) & left_of(Y,Z) & on(Z,Y) ) => ( false)).
penalty(2163, 1) <- ~( !X, Y, Z : ( food_container(X) & left_of(X,Y) & left_of(Y,Z) ) => ( food_utensil(Z))).
penalty(2164, 1) <- ~( !X, Y, Z : ( food_container(X) & left_of(X,Y) & left_of(Y,Z) & food_container(Z) ) => ( false)).
penalty(2165, 1) <- ~( !X, Y, Z : ( food_container(X) & left_of(X,Y) & left_of(Y,Z) & drink_container(Z) ) => ( false)).
penalty(2166, 1) <- ~( !X : ( drink_container(X) ) => ( waterGlass(X) | wineGlass(X) | teaCup(X))).
penalty(2167, 1) <- ~( !X, Y : ( drink_container(X) & left_of(X,Y) ) => ( wineGlass(X) | teaCup(X))).
}
}



structure S : V {

    fork = {}
    knife = {knife}
    spoon = {}
    plate = {plate}
    bowl = {}
    soupPlate = {}
    teaCup = {cup}
    waterGlass = {}
    wineGlass = {}

    RuleNr = {0..2298}
    Penalty = {0..1}

    // above_of = {(f, water)}

    PossCo = {0..3}
    // Example 1 in claudien:
    // position = {(0, 0, 0, fork); (1, 0, 0, plate); (2, 0, 0, knife); (2, 1, 0, water)}
    
    // Example 2 in claudien:
    // position = {(0, 0, 0, fork); (1, 0, 0, plate); (2, 0, 0, knife); (2, 1, 0, wine)}

    // Example 3 in claudien:
    // position = {(0, 0, 0, plate); (1, 0, 0, fork); (2, 0, 0, knife); (2, 1, 0, wine)}

    // Not an example but should work with penalty/penalty
    // position = {(0, 0, 0, plate); (1, 0, 0, fork); (2, 0, 0, water); (2, 1, 0, spoon)}

    // Example 1 (not learned)
    position = {(0, 0, 0, plate); (1, 0, 0, knife); (1, 1, 0, cup)}

    // Breakfast 3
    // position = {(0, 0, 0, fork); (1, 0, 0, plate); (2, 0, 0, knife); (2, 1, 0, cup); (3, 1, 0, water)}

    // Breakfast: should give unsatisfiable. Because there is a waterGlass and teaCup... (is important rule, without penalty)
    // position = {(0, 0, 0, plate); (0, 0, 1, bowl); (1, 0, 0, knife); (2, 1, 0, cup); (3, 0, 0, spoon); (3, 1, 0, water)}

}

procedure main() {
    stdoptions.nrpropsteps=1
    // stdoptions.longestbranch=1
    stdoptions.liftedunitpropagation=false
    stdoptions.cpsupport=true
    // stdoptions.verbosity.grounding=1
    stdoptions.verbosity.solving=1
    stdoptions.groundwithbounds=false
    stdoptions.splitdefs=false

    sol = minimize(T,S,penaltyTerm)[1]
    print(sol[V::penalty].ct)
}






