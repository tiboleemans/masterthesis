penalty(186, 1) <- ~(( there_is_a_pastaPlate & there_is_a_teaCup ) => ( false)).
penalty(187, 1) <- ~( !X, Y : ( there_is_a_pastaPlate & on(X,Y) ) => ( false)).
penalty(188, 1) <- ~(( there_is_a_soupPlate & there_is_a_teaCup ) => ( false)).
penalty(189, 1) <- ~(( there_is_a_wineGlass & there_is_a_teaCup ) => ( false)).
penalty(190, 1) <- ~( !X, Y : ( above_of(X,Y) & on(X,Y) ) => ( false)).
